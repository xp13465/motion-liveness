    "react-test-renderer": "16.13.1",
    "typescript": "^3.9.5"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
