{
  "name": "react-native-motion-liveness",
  "version": "0.1.0",
  "description": "原生视频认证组件",
  "main": "src/index.tsx",
  "react-native": "src/index.tsx",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "react-native-motion-liveness.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd ios && pod-install --quiet",
    "iosBundle": "react-native bundle --entry-file ./src/index.tsx --bundle-output ./ios/bundle/main.jsbundle --platform ios --assets-dest ./ios/bundle --dev false",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git@code.lianai001.me:app/motion-liveness.git"
  },
  "author": "柳广剑 <liuguangjian@lianai001.me> (https://code.lianai001.me/app/motion-liveness)",
  "license": "MIT",
  "bugs": {
    "url": "https://code.lianai001.me/app/motion-liveness/issues"
  },
  "homepage": "https://code.lianai001.me/app/motion-liveness#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
